@page
@model Amphora.Api.Areas.Organisations.Pages.Accounts.DebitsModel

@{
    ViewData["Title"] = "Org Debits";
}

<div class="container">
    <div class="row">
        <div class="col">
            <h2>Debits</h2>
            <hr/>
            @foreach(var d in Model.Account.Debits)
            {
                @await Component.InvokeAsync("TransactionRow", new {transaction = d, size = "medium" })
            }
        </div>
    </div>
</div>
