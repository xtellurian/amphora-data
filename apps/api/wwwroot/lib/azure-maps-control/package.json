{
  "name": "azure-maps-control",
  "author": "Microsoft Corporation",
  "version": "2.0.20",
  "description": "Map SDK for Azure Maps",
  "keywords": [
    "azure",
    "maps",
    "ui",
    "map",
    "control",
    "lbs",
    "microsoft"
  ],
  "scripts": {
    "build": "node ./bin/build.js",
    "docs": "(if exist \"./docs/\" rmdir \"./docs/\" /S /Q) && tsc --declaration --declarationDir ./docs --emitDeclarationOnly --stripInternal",
    "lessc": "node_modules/.bin/lessc",
    "test": "set \"TS_NODE_PROJECT=./test/tsconfig.json\" && mocha-parallel-tests --exit --timeout 30000 --require ts-node/register test/**/*.ts",
    "test-filter": "set \"TS_NODE_PROJECT=./test/tsconfig.json\" && mocha-parallel-tests --exit --timeout 30000 --require ts-node/register",
    "thirdPartyNotices": "node ./bin/thirdPartyNotices.js",
    "tslint": "node_modules/.bin/tslint --project ./tsconfig.json",
    "updateEnvVariables": "node ./bin/updateEnvVariables.js",
    "updateExampleKeys": "node ./bin/updateExampleKeys.js"
  },
  "dependencies": {
    "@types/adal-angular": "^1.0.1"
  },
  "devDependencies": {
    "@mapbox/mapbox-gl-rtl-text": "0.1.2",
    "@microsoft/applicationinsights-web": "^2.2.2",
    "@types/color": "^3.0.0",
    "@types/jwt-decode": "^2.2.0",
    "@types/lodash": "4.14.136",
    "@types/mapbox-gl": "^0.54.5",
    "@types/mocha": "^5.2.7",
    "@types/puppeteer": "^1.19.1",
    "adal-angular": "^1.0.17",
    "color": "^3.1.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "grunt": "^1.0.4",
    "jwt-decode": "^2.2.0",
    "less": "^3.9.0",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "4.17.15",
    "mapbox-gl": "^1.4.1",
    "mocha": "^6.2.0",
    "mocha-parallel-tests": "^2.2.2",
    "node-fetch": "^2.2.0",
    "puppeteer": "^1.19.0",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.7.2",
    "uuid-random": "^1.0.9",
    "yargs": "^13.3.0"
  },
  "license": "SEE LICENSE IN LICENSE.TXT",
  "files": [
    "typings/**/*",
    "dist/**/*",
    "thirdpartynotices.txt"
  ],
  "types": "./typings/index.d.ts",
  "main": "./dist/atlas.min.js"
}
