{
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Error"
      }
    },
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IdentityConfiguration": {
    "SignIn": {
      "RequireConfirmedEmail": "false"
    }
  },
  "tokenManagement": {
    "issuer": "amphoradata.com",
    "audience": "amphoradata,com",
    "accessExpiration": 30,
    "refreshExpiration": 60
  },
  "featureFlags": {
    "signals": "enabled"
  },
  "SendGrid": {
    "FromEmail": "contact@amphoradata.com",
    "FromName": "The Amphora Data Team"
  },
  "Chat": {
    "RedirectUrl": "https://amphoradata.slack.com"
  },
  "Feedback": {
    "RedirectUrl": "https://github.com/amphoradata/amphoradata.github.io/issues/new"
  },
  "Signals": {
    "MaxSignals": 7
  },
  "AmphoraManagement": {
    "SoftDelete": true,
    "DeletedTimeToLive": 172800
  }
}
